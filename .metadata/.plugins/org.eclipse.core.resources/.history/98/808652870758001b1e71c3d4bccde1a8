				paciente.setCPF("491.375.858-03");
				paciente.setEmail("aluis2085@gmail.com");
				
			}
				
			
			
			
			
			
		};
		

					};
			
		
		
		
	


		
	public static void main(String[] args) {
		SpringApplication.run(TodoApplication.class, args);
		
	
	}

}
